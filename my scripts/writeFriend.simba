// press specific location
function TIncubate.PlantSeedsInPot:Boolean;
var
  seedItemSlot,potItemSlot,startingCount:Int32;
begin
  startingCount:= Inventory.Items.CountStack(self.Seeds[0]); //caches our staring seed count
  Inventory.FindItem(self.Seeds[0],seedItemSlot); //Stores our seed slot to seedItemSlot for clicking
  Inventory.FindItem(self.Pots[0],potItemSlot); // Stores our filled plant slot to potItemSlot for clicking

  Inventory.ClickSlot(potItemSlot,''); //clicks our pot first
  Wait(350,458);
  Result:=Inventory.ClickSlot(seedItemSlot,'Use Filled plant pot -> '+self.Seeds[0]);//clicks our seeds to start planting
  Wait(10,25);
  WaitUntil((self._CountSeeds =(startingCount-25)),5000,50000);// This sleeps our bot. Compares starting seed cache count to running count with _CountSeeds.
end; 