  TAshes = record
    Points: TPointArray;
    Items: TRSItemArray;
    Objects: TRSObjectArray;
    Int: Int32;
    BankItem: TRSBankItem;
  end;
procedure TAshes.Init;
begin
  Map.SetupChunk(Chunk([48,55,50,53], 0));

  SetLength(self.Items,6);
  self.Items[0]:='Vial ashes';
  self.Items[1]:='Fiendish ahses';
  self.Items[2]:='Malicious ashes';
  self.Items[3]:='Abbysal ashes';
  self.Items[4]:='Infernal ashes';

  self.Int:= 0;//sets the integer for the type of ashes you load.

  self.BankItem.Item:= self.Items[self.Int];
  self.BankItem.Noted:= False;
  self.BankItem.Quantity:= -1;

  WriteLn self.BankItem;
end;

procedure TAshes.TakeItemsFromBank;
begin
  if bank.IsOpen(20,200) then
  Bank.WithdrawItem(self.BankItem,True);
end;

var
  Ashes:TAshes;
begin
  Ashes.Init;
  Ashes.TakeItemsFromBank;
end;

